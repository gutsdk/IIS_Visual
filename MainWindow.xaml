<Window x:Class="IIS_Visual.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IIS_Visual"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        mc:Ignorable="d"
        Title="Лабораторная работа по ИИС" Height="600" Width="800" ResizeMode="CanResize">

    <Grid>
        <lvc:CartesianChart Name="Chart_1" Series="{Binding seriesCollection}" LegendLocation="Right"/>
    </Grid>
</Window>
